@using FD_CPTP_UNIFIED;

@{
    Layout = null;
    string CDNRootPath = Startup.Configuration["AppCssCDNPath"].ToString();
}
<!DOCTYPE html>

<html>
<head>

    <meta charset="utf-8" meta http-equiv="X-UA-Compatible" content="IE=edge" name="viewport">
    <title id="MahindraFinance">Mahindra Finance Fixed Deposit Channel Partner Support Application</title>
    <link rel="shortcut icon" href="@String.Concat(CDNRootPath,"favicon.ico")" type="image/x-icon" asp-append-version="true" />
    <link rel="icon" href="@String.Concat(CDNRootPath,"favicon.ico")" type="image/x-icon" asp-append-version="true" />
    <link rel="stylesheet" type="text/css" href="@String.Concat(CDNRootPath,"css/fonts.min.css")" asp-append-version="true" />

    <link rel="stylesheet" type="text/css" href="@String.Concat(CDNRootPath,"css/basic.min.css")" asp-append-version="true" />
    <link rel="stylesheet" type="text/css" href="@String.Concat(CDNRootPath,"css/applayout.min.css")" asp-append-version="true" />
    <style>
        .content-wrapper .options {
            box-shadow: 0px 1px 4px #dbdbdb;
        }
    </style>

    @*@RenderSection("Styles", required: false)*@

    <script type="text/javascript" src="@String.Concat(CDNRootPath,"js/jquery.min.js")" asp-append-version="true"></script>

</head>
<body class="hold-transition fixed">

    <div class="wrapper">
        <header class="main-header">
            <nav class="navbar navbar-default">
                <div class="container-fluid">
                    <!-- Brand and toggle -->
                    <div class="navbar-header">
                        <i class="demo-icon icon-menu navbar-toggle collapsed pull-left" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">&#xf0c9;</i>
                        <a class="navbar-brand" href="#">
                            <img alt="MF-Logo" src="@String.Concat(CDNRootPath,"images/MF_Logo_Horizontal4.svg")" class="img-responsive" />
                        </a>
                    </div>

                    <!-- Desktop Profile -->
                    <div class="collapse navbar-collapse desktop-profile">
                        <ul class="nav navbar-nav navbar-right">
                            @*<li>
                                    <a id="lnkHome" asp-area="" asp-controller="Home" asp-action="Home"><i class="demo-icon icon-home">&#xe802;</i>Home</a>
                                </li>*@
                            <li>
                                <a id="lnkLogout" asp-area="" asp-controller="Home" asp-action="LogOut"><i class="demo-icon icon-off">&#xe804;</i>Logout</a>
                            </li>
                        </ul>
                    </div>
                </div>
            </nav>

            <!-- Second Navigation -->
            <nav class="navbar second-navbar">
                <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                    <!-- Mobile Profile -->
                    <ul class="nav mobile-profile">
                        @*<li>
                                <a asp-area="" asp-controller="Home" asp-action="Home"><i class="demo-icon icon-home">&#xe802;</i>Home</a>
                            </li>*@
                        <li>
                            <a asp-area="" asp-controller="Home" asp-action="LogOut"><i class="demo-icon icon-off">&#xe804;</i>Logout</a>
                        </li>
                    </ul>
                </div>
            </nav>
        </header>

        <div class="content-wrapper">
            <div class="container page-body">
                <p style="font-size: 100px; color: #a1a1a1;">
                    Session Timeout
                </p>
                <p style="font-size: 25px; color: #6d6e70;">
                    Sorry, your session has timed out, sign in again
                </p>
            </div>
        </div>

        <footer class="main-footer">
            <div class="container-fluid">
                <p class="pull-left">&copy; 2018 Mahindra Finance</p>
            </div>
        </footer>
    </div>

    <script src="@String.Concat(CDNRootPath,"js/applayout.min.js")"></script>
    <script src="@String.Concat(CDNRootPath,"js/app.js")"></script>

</body>
</html>

