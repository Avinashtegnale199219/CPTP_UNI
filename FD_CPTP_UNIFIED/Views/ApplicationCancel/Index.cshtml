    <div id="DV_DATAENTRY" class="mainBodyOutr" style="display:none">

        @{await Html.RenderPartialAsync("~/Views/Partial_View/_Header.cshtml");}


        <div class="tab-content NavTabsCont" id="allTabsContainer">
