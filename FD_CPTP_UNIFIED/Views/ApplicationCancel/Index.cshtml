                    <div class="form-group FormGrpCustm">
                        <label class="control-label">Application Number</label>
                        <input type="text" id="txtSearchApplicationToCancel" class="form-control  uppercase" />

                    </div>
                </div>
                <div class="col-md-1 col-sm-3 col-xs-12 InpOutrCls">
                    <button type="button" id="btnSearchApplicationToCancel" class="comnBtn">Search</button>
                </div>
            </div>
