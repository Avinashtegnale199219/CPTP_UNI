<div id="DV_FATCA_DETAILS">
    <h4 class="SecHeaderTxt" style="padding-left:5px;">Detail Entry</h4>
    <div class="col-md-2 col-sm-4 col-xs-12 InpOutrCls">
        <div class="form-group FormGrpCustm">
            <label class="control-label">Country of Tax<span class="red-text">*</span></label>
            <select id="ddlCountryOfTax" name="COUNTRY_OF_TAX" val-type="select" class="form-control F fatca" errmsg="Country of Tax is required..!">
                <option value="select">Select</option>
            </select>
            <span id="errorCountry" class="help-block text-danger"></span>
        </div>
    </div>
    <div class="col-md-2 col-sm-4 col-xs-12 InpOutrCls">
        <div class="form-group FormGrpCustm">
            <label class="control-label">Tax Identification Type<span class="red-text">*</span></label>
            <select id="ddlTaxIndentityType" name="TAX_IDENTIFICATION_TYPE" class="form-control F fatca " errmsg="TaxIdentification Type is required..!" val-type="select">
                <option value="select">Select</option>
            </select>
            <span id="errorTaxIndentityType" class="help-block text-danger"></span>
        </div>
    </div>
    <div class="col-md-2 col-sm-4 col-xs-12 InpOutrCls">
        <div class="form-group FormGrpCustm">
            <label class="control-label">Tax Identification No<span class="red-text">*</span></label>
            <input type="text" id="txttaxIdentityNo" class="form-control F NoSpecialChar fatca number_only" errmsg="Tax Identification Number is required..!" val-type="text">
            <span id="errortaxIdentityNo" class="help-block text-danger"></span>
        </div>
    </div>
    <div class="col-md-2 col-sm-4 col-xs-12 InpOutrCls">
        <div class="form-group FormGrpCustm SearchDate" id="DobSearch">
            <label class="control-label">TRC Expiry Date<span class="red-text">*</span></label>
            <input type="text" id="txtexpTRCDate" name="TRC_EXP_DATE" val-type="text" required regex="" errmsg="TRC Expiry Date is required..!" class="form-control D_T_E fatca">
            <span id="errorexpTRCDate" class="help-block text-danger"></span>
        </div>
    </div>
    <div class="col-md-2 col-sm-4 col-xs-12 InpOutrCls">
        <div class="form-group FormGrpCustm">
            <label class="control-label">Address Type<span class="red-text">*</span></label>
            <select id="ddlAddressType" name="ADDRESS_TYPE" class="form-control F fatca address" errmsg="Address Type is required..!" val-type="select">
                <option value="select">Select</option>

            </select>
            <span id="errorAddressType" class="help-block text-danger"></span>
        </div>
    </div>
    <div class="col-md-4 col-sm-4 col-xs-12 InpOutrCls">
        <div class="form-group FormGrpCustm">
            <label class="control-label">Address Line 1<span class="red-text">*</span></label>
            <input type="text" id="txtFatcaAdd1" maxlength="40" rows="2" val-type="text" errmsg="Address1 is required..!" class="form-control uppercase F NoSpecialChar TextAreaInput fatca address RemoveSpace" aria-autocomplete="none" />
            <span id="errorAddress1" class="help-block text-danger"></span>
        </div>
    </div>
    <div class="col-md-4 col-sm-4 col-xs-12 InpOutrCls">
        <div class="form-group FormGrpCustm">
            <label class="control-label">Address Line 2<span class="red-text">*</span></label>
            <input type="text" id="txtFatcaAdd2" rows="2" maxlength="40" val-type="text" class="form-control F NoSpecialChar TextAreaInput fatca address uppercase RemoveSpace" errmsg="Address2 is required..!" aria-autocomplete="none" />
            <span id="errorAddress2" class="help-block text-danger"></span>
        </div>
    </div>
    <div class="col-md-4 col-sm-4 col-xs-12 InpOutrCls">
        <div class="form-group FormGrpCustm">
            <label class="control-label">Landmark</label>
            <input type="text" id="txtFatcaLandmark" rows="2" val-type="text" maxlength="40" class="form-control F NoSpecialChar TextAreaInput fatca uppercase RemoveSpace" errmsg="Landmark is required..!" aria-autocomplete="none" />
            <span id="errorLandmark" class="help-block text-danger"></span>
        </div>
    </div>
    <div class="col-md-2 col-sm-4 col-xs-12 InpOutrCls">
        <div class="form-group FormGrpCustm">
            <label class="control-label">State<span class="red-text">*</span></label>
            <select id="ddlFatcaState" class="form-control F fatca" val-type="ddlModalDetal" errmsg="State is required..!"></select>
            <span id="errorStateBind" class="help-block text-danger"></span>
        </div>
    </div>
    <div class="col-md-2 col-sm-4 col-xs-12 InpOutrCls">
        <div class="form-group FormGrpCustm">
            <label class="control-label">City<span class="red-text">*</span></label>
            <input type="text" id="txtFatcaCity" maxlength="10" class="form-control F NoSpecialChar fatca charactor_only uppercase RemoveSpace" val-type="text" errmsg="City is required..!">
            <span id="errorCity" class="help-block text-danger"></span>
        </div>
    </div>
    <div class="col-md-2 col-sm-4 col-xs-12 InpOutrCls">
        <div class="form-group FormGrpCustm">
            <label class="control-label">Postal Code<span class="red-text">*</span></label>
            <input type="text" id="txtFatcaPostalcode" maxlength="7" class="form-control F NoSpecialChar RemoveSpace fatca" errmsg="Postal Code is required..!" val-type="txtModalDetal">
            <span id="errorPostalcode" class="help-block text-danger"></span>
        </div>
    </div>
    <div class="col-md-2 col-sm-4 col-xs-12 InpOutrCls">
        <div class="form-group FormGrpCustm">
            <label class="control-label">Primary STD Code</label>
            <input type="text" id="txtSTDCodePrimary" maxlength="7" class="form-control F NoSpecialChar number_only RemoveSpace" errmsg="STD Code is required..!" val-type="txtModalDetal">
            <span id="errorSTDCodePrimary" class="help-block text-danger"></span>
        </div>
    </div>
    <div class="col-md-2 col-sm-4 col-xs-12 InpOutrCls">
        <div class="form-group FormGrpCustm">
            <label class="control-label">Primary Telephone No</label>
            <input type="text" maxlength="10" id="txtTelephoneNumberPrimary" class="form-control F NoSpecialChar RemoveSpace number_only " errmsg="Primary Telephone Number is required..!" val-type="txtModalDetal" />
            <span id="errorTelephoneNumberPrimary" class="help-block text-danger"></span>
        </div>
    </div>
    <div class="col-md-2 col-sm-4 col-xs-12 InpOutrCls">
        <div class="form-group FormGrpCustm">
            <label class="control-label">Primary Mobile no<span class="red-text">*</span></label>
            <input type="text" id="txtMobileNumberPrimary" name="MOBILE" maxlength="10" class="form-control F fatca NoSpecialChar RemoveSpace number_only mobileF" errmsg="Primary Mobile Number is required..!" val-type="txtModalDetal" />
            <span class="errorbox" style="position: absolute; top: 3px; left: 166px;display:none"></span>
        </div>
    </div>
    <div class="col-md-2 col-sm-4 col-xs-12 InpOutrCls">
        <div class="form-group FormGrpCustm">
            <label class="control-label">Other STD Code</label>
            <input type="text" id="txtSTDCodeOther" class="form-control F NoSpecialChar number_only RemoveSpace" errmsg="STD Code is required..!" val-type="txtModalDetal" />
            <span id="errorSTDCodeOther" class="help-block text-danger"></span>
        </div>
    </div>
    <div class="col-md-2 col-sm-4 col-xs-12 InpOutrCls">
        <div class="form-group FormGrpCustm">
            <label class="control-label">Other Telephone No</label>
            <input type="text" maxlength="18" id="txtTelephoneNumberOther" class="form-control F NoSpecialChar number_Only RemoveSpace " errmsg="Telephone Number is required..!" val-type="txtModalDetal" />
            <span id="errorTelephoneNumberOther" class="help-block text-danger"></span>
        </div>
    </div>
    <div class="col-md-2 col-sm-4 col-xs-12 InpOutrCls">
        <div class="form-group FormGrpCustm">
            <label class="control-label">Other Mobile no</label>
            <input type="text" id="txtMobileNumberOther" name="MOBILE" maxlength="10" class="form-control F NoSpecialChar number_only RemoveSpace mobileF " errmsg="Mobile Number is required..!" val-type="txtModalDetal" />
            <span class="errorbox" style="position: absolute; top: 3px; left: 166px;display:none"></span>
        </div>
    </div>
    <div class="col-md-2 col-sm-3 col-xs-12 InpOutrCls">
        <button type="button" id="btnBind" class="comnBtn BIND_DETAIL">Add Detail</button>
        <button type="button" id="btnFatacClear" class="comnBtn CLEAR_DETAIL">Clear</button>
    </div>
</div>

